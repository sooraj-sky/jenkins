pipeline {
    agent any

    stages {
        stage('Run Shell Script') {
            steps {
                script {
                    // Execute the shell script with arguments env1 and env2
                    def one = "test1"
                    def two = "test2"
                    sh """
                      chmod +x test.sh
                    """
                    def scriptOutput = sh(returnStdout: true, script: './test.sh $one $two').trim()
                    echo "Concatenated Output: ${scriptOutput}"
                }
            }
        }
    }
}
